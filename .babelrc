{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "> 1%",
          "Chrome >= 50",
          "IE >= 11",
          "Edge >= 14",
          "Firefox >= 50",
          "Opera >= 40",
          "Safari >= 8",
          "iOS >= 8"
        ]
      },
      "useBuiltIns": false
    }]
  ],

  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
